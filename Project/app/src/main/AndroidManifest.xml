<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tr.edu.yildiz.kaankesgin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/q7"
        android:label="Quiz"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homework">
        <activity android:name=".setExamActivity"></activity>
        <activity android:name=".quizSettingsActivity" />
        <activity android:name=".photoPickerActivity" />
        <activity android:name=".popUpActivity" />
        <activity android:name=".UpdateActivity" />
        <activity android:name=".listQuestions" />
        <activity android:name=".addQuestionActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>